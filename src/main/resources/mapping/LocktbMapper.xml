<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ceilan.dao.LocktbMapper" >
  <resultMap id="BaseResultMap" type="com.ceilan.entity.Locktb" >
    <result column="kid" property="kid" jdbcType="INTEGER" />
    <result column="name" property="name" jdbcType="VARCHAR" />
    <result column="age" property="age" jdbcType="BIGINT" />
  </resultMap>
  <insert id="insert" parameterType="com.ceilan.entity.Locktb" >
    insert into locktb (kid, name, age
      )
    values (#{kid,jdbcType=INTEGER}, #{name,jdbcType=VARCHAR}, #{age,jdbcType=BIGINT}
      )
  </insert>
  <insert id="insertSelective" parameterType="com.ceilan.entity.Locktb" >
    insert into locktb
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="kid != null" >
        kid,
      </if>
      <if test="name != null" >
        name,
      </if>
      <if test="age != null" >
        age,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="kid != null" >
        #{kid,jdbcType=INTEGER},
      </if>
      <if test="name != null" >
        #{name,jdbcType=VARCHAR},
      </if>
      <if test="age != null" >
        #{age,jdbcType=BIGINT},
      </if>
    </trim>
  </insert>
  <update id="delLocktb" parameterType="Long">
      update locktb set name=CONCAT('name',#{age}) where age=100
  </update>
</mapper>