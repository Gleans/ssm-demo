<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ceilan.dao.ClassMapper" >
  <resultMap id="BaseResultMap" type="com.ceilan.entity.Class" >
    <result column="id" property="id" jdbcType="INTEGER" />
    <result column="name" property="name" jdbcType="VARCHAR" />
    <result column="score" property="score" jdbcType="DOUBLE" />
    <result column="kemu" property="kemu" jdbcType="VARCHAR" />
  </resultMap>
  <resultMap id="classResultMap" type="map">
    <result column="id" property="key" jdbcType="INTEGER" />
    <result column="name" property="value" jdbcType="VARCHAR" />
  </resultMap>
  <select id="queryClassAll" resultMap="classResultMap">
    select id,name from class
  </select>
  <insert id="insert" parameterType="com.ceilan.entity.Class" >
    insert into class (id, name, score, 
      kemu)
    values (#{id,jdbcType=INTEGER}, #{name,jdbcType=VARCHAR}, #{score,jdbcType=DOUBLE}, 
      #{kemu,jdbcType=VARCHAR})
  </insert>
  <insert id="insertSelective" parameterType="com.ceilan.entity.Class" >
    insert into class
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="id != null" >
        id,
      </if>
      <if test="name != null" >
        name,
      </if>
      <if test="score != null" >
        score,
      </if>
      <if test="kemu != null" >
        kemu,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="id != null" >
        #{id,jdbcType=INTEGER},
      </if>
      <if test="name != null" >
        #{name,jdbcType=VARCHAR},
      </if>
      <if test="score != null" >
        #{score,jdbcType=DOUBLE},
      </if>
      <if test="kemu != null" >
        #{kemu,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
</mapper>